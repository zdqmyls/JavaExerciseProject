<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AccountSystem.view.lineChartFrameController">
   <children>
      <HBox alignment="CENTER" prefHeight="68.0" prefWidth="800.0">
         <children>
            <ComboBox fx:id="lineChart_comboBox" onAction="#lineChart_comboBoxEvent" promptText="请选择日期" />
         </children>
      </HBox>
      <ScrollPane fitToHeight="true" fitToWidth="true" layoutY="74.0" pannable="true" prefHeight="725.0" prefWidth="800.0">
         <content>
            <LineChart fx:id="lineChart">
              <xAxis>
                <CategoryAxis fx:id="categoryAxis" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="numberAxis" side="LEFT" />
              </yAxis>
            </LineChart>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
